{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --host",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "start:prod": "node -r dotenv/config .output/server/index.mjs",
    "ionic:build": "yarn ionic:gen-icon && yarn generate",
    "ionic:serve": "yarn dev",
    "ionic:dev": "ionic cap run --external -livereload --consolelogs",
    "ionic:gen-icon": "npx @capacitor/assets generate --iconBackgroundColor #ffffff --iconBackgroundColorDark #000000 --splashBackgroundColor #ffffff --splashBackgroundColorDark #000000 --logoSplashScale 0.1 --assetPath assets/resources"
  },
  "dependencies": {
    "@capacitor/android": "^5.7.4",
    "@capacitor/app": "^5.0.7",
    "@capacitor/assets": "^3.0.4",
    "@capacitor/cli": "^5.7.4",
    "@capacitor/core": "^5.7.4",
    "@capacitor/geolocation": "^6.0.0",
    "@capacitor/screen-orientation": "^5.0.7",
    "@capacitor/status-bar": "github:ramlan442/capacitor-status-bar-height",
    "@flaticon/flaticon-uicons": "^3.1.0",
    "@hugotomazi/capacitor-navigation-bar": "^3.0.0",
    "@maptiler/leaflet-maptilersdk": "^2.0.0",
    "@mdi/font": "^7.4.47",
    "@nuxtjs/svg-sprite": "^1.0.2",
    "@pinia/nuxt": "^0.5.1",
    "@types/leaflet": "^1.9.11",
    "@types/leaflet-routing-machine": "^3.2.8",
    "animate.css": "^4.1.1",
    "axios": "^1.6.8",
    "fullscreen": "^1.1.1",
    "leaflet": "^1.9.4",
    "leaflet-routing-machine": "^3.2.12",
    "node-forge": "^1.3.1",
    "nuxt": "^3.11.1",
    "socket.io-client": "^4.7.5",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@pinia-plugin-persistedstate/nuxt": "^1.2.0",
    "@types/node-forge": "^1.3.11",
    "@vite-pwa/nuxt": "^0.6.0",
    "sass": "^1.72.0",
    "typescript": "^5.4.3",
    "vuetify-nuxt-module": "^0.12.0"
  }
}
